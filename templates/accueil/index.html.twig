{% extends 'base.html.twig' %}

{% block title %}Accueil{% endblock %}

{% block body %}

<div class="row col">
    <h1>Accueil</h1>
</div>


    <div id="carouselExampleControls" class="carousel slide mb-3 col-lg-8 mx-auto" data-ride="carousel">
        <div class="carousel-inner">
            <div class="carousel-item active">
            <img class="d-block w-100" src="https://www.officeeasy.fr/guides/amplificateur-gsm/include/images/ampgsm121.jpg" alt="First slide">
            </div>
            <div class="carousel-item">
            <img class="d-block w-100" src="https://bulletindescommunes.net/wp-content/uploads/2021/01/orang-735x400.jpg.webp" alt="Second slide">
            </div>
            <div class="carousel-item">
            <img class="d-block w-100" src="https://www.lexpress.fr/resizer/v2/EFSFPWPFBVDXVGWHDFQDFROXVY.jpg?auth=0ee13792cb23d881dc197f994094cbacf5b4eddaf518f62079f6f7025b819349&width=1200&height=630&quality=85&smart=true" alt="Third slide">
